                    "<PAUSE>",
                    "<FADE_OUT>black,670",
                    "<EV>r18_120835_000,NONE,0",
                    "<FADE_IN>black,670",
                    "<VOICE_PLAY>fkg_120835_hscene005",
                    "<NAME_PLATE>赛菊芋",
                    "「啊，唔唔……！」",
                    "<PAUSE>",
