            "PART1": {
                "SCRIPT": [
                    "<BGM_PLAY>fkg_bgm_hscene001,1000",
                    "<VOICE_PLAY>fkg_125503_hscene001",
                    "<NAME_PLATE>马缨丹",
                    "「嘿咻……」",
                    "<PAUSE>",
                    "<NAME_PLATE>",
